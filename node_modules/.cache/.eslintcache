Name=\"mb-3\" controlId=\"formBasicPassword\">\n        <Form.Label>Password</Form.Label>\n        <Form.Control\n          type=\"password\"\n          placeholder=\"Password\"\n          value={password}\n          onChange={(event) => setPassword(event.target.value)}\n        />\n      </Form.Group>\n      <Form.Group className=\"mb-3\" controlId=\"formConfirmPassword\">\n        <Form.Label>Confirm your Password</Form.Label>\n        <Form.Control\n          type=\"password\"\n          placeholder=\"Password Confirmation\"\n          value={confirmPassword}\n          onChange={(event) => setConfirmPassword(event.target.value)}\n        />\n      </Form.Group>\n      <Button variant=\"primary\" type=\"submit\">\n        Signup\n      </Button>\n      <Button variant=\"white\" type=\"button\" className=\"btn plain-btn\" onClick={() => navigate('/login')}>\n        login\n      </Button>\n    </Form>\n  );\n};\n\nexport default SignupForm;\n","/home/juhar/Desktop/microverse-projects/Booking-health-service/src/components/sidebar/SideBar.jsx",[],[],"/home/juhar/Desktop/microverse-projects/Booking-health-service/src/pages/services/Services.jsx",[],[],"/home/juhar/Desktop/microverse-projects/Booking-health-service/src/components/sidebar/SocialLinks/SocialLinks.jsx",[],[],"/home/juhar/Desktop/microverse-projects/Booking-health-service/src/components/sidebar/Links/Links.jsx",[],[],"/home/juhar/Desktop/microverse-projects/Booking-health-service/src/components/sidebar/SidebarData.js",[],[],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","replacedBy":"97"},{"ruleId":"98","severity":1,"message":"99","line":32,"column":6,"nodeType":"100","endLine":32,"endColumn":15,"suggestions":"101"},"lines-around-directive",["102"],"no-spaced-func",["103"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"react-hook